{
	"entryPoints": [
		"./src/compiler/compiler-options.ts",
		"./src/compiler/runtime/type.ts",
		"./src/compiler/server/type.ts"
	],
	"out": "./docs-api",
	"plugin": ["typedoc-plugin-missing-exports", "typedoc-plugin-markdown"],
	"tsconfig": "tsconfig.json",
	"outputFileStrategy": "modules",
	"readme": "none",
	"excludeExternals": true,
	"hidePageHeader": true,
	"hideBreadcrumbs": true,
	"hideGroupHeadings": true,
	"hidePageTitle": true,
	"sourceLinkTemplate": "https://github.com/opral/monorepo/tree/main/{path}"
}
