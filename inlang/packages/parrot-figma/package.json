{
	"name": "@inlang/parrot-figma",
	"version": "0.10.100",
	"private": true,
	"description": "Plugin to handle assets and resources directly within figma",
	"main": "code.js",
	"type": "module",
	"engines": {
		"node": ">=20",
		"pnpm": ">=10 <11"
	},
	"scripts": {
		"dev": "vite",
		"build": "PLUGIN_CODE_BASE_URL=${RENDER_EXTERNAL_URL} vite build",
		"tsc": "tsc --noEmit",
		"generate-strings": "node ./util/assets-and-resources.js;",
		"start": "set -a; source .env; set +a; webpack --mode development --watch ",
		"start-with-server": "node ./util/assets-and-resources.js; webpack --mode development --watch & npx http-server --cors -o ./dist",
		"lint": "eslint .",
		"lintfix": "./node_modules/.bin/eslint . --fix ",
		"format": "prettier ./src --write"
	},
	"author": "Martin Lysk",
	"license": "",
