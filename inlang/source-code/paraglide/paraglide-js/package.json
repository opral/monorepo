{
	"name": "@inlang/paraglide-js",
	"type": "module",
	"version": "1.6.1",
	"license": "Apache-2.0",
	"publishConfig": {
		"access": "public"
	},
	"author": "inlang <hello@inlang.com> (https://inlang.com/)",
	"homepage": "https://inlang.com/m/gerre34r/library-inlang-paraglideJs",
	"repository": {
		"type": "git",
		"url": "https://github.com/opral/inlang-paraglide-js"
	},
	"keywords": [
		"paraglide",
		"javascript i18n",
		"i18n",
		"l10n",
		"translation",
		"internationalization",
		"svelte",
		"localization",
		"lint",
		"react",
		"vue",
		"angular",
		"nextjs",
		"next i18n",
		"astro",
		"astro i18n",
		"solid",
		"solidstart"
	],
	"bin": {
		"paraglide-js": "./bin/run.js"
	},
	"files": [
		"./dist",
		"./bin",
		"./default"
	],
	"scripts": {
		"dev": "vite build --mode development --watch",
		"build": "vite build --mode production",
		"test": "tsc --noEmit --emitDeclarationOnly false && vitest run --coverage ./src/**/*",
		"lint": "eslint ./src --fix",
		"format": "prettier ./src --write",
		"clean": "rm -rf ./dist ./node_modules"
	},
	"dependencies": {
		"@inlang/detect-json-formatting": "workspace:*",
		"commander": "11.1.0",
		"consola": "3.2.3",
		"dedent": "1.5.1",
		"json5": "2.2.3",
		"posthog-node": "3.1.3"
	},
	"devDependencies": {
		"@inlang/cross-sell-sherlock": "workspace:*",
		"@inlang/env-variables": "workspace:*",
		"@inlang/language-tag": "workspace:*",
		"@inlang/plugin-message-format": "workspace:*",
		"@inlang/sdk": "workspace:*",
		"@inlang/telemetry": "workspace:*",
		"@lix-js/client": "workspace:*",
		"@lix-js/fs": "workspace:*",
		"@rollup/plugin-terser": "0.4.3",
		"@rollup/plugin-virtual": "3.0.1",
		"@ts-morph/bootstrap": "0.20.0",
		"@types/minimist": "1.2.3",
		"@types/node": "^20.11.4",
		"@vitest/coverage-v8": "0.34.3",
		"memfs": "4.6.0",
		"rollup": "3.29.1",
		"typescript": "5.2.2",
		"vite": "4.5.2",
		"vite-plugin-dts": "^3.8.1",
		"vite-tsconfig-paths": "^4.3.2",
		"vitest": "0.34.3"
	},
	"exports": {
		".": {
			"import": "./default/index.js",
			"types": "./default/index.d.ts"
		},
		"./internal": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./internal/adapter-utils": {
			"import": "./dist/adapter-utils/index.js",
			"types": "./dist/adapter-utils/index.d.ts"
		}
	}
}