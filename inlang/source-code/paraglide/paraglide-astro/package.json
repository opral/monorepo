{
	"name": "@inlang/paraglide-astro",
	"version": "0.4.1",
	"author": "inlang <hello@inlang.com> (https://inlang.com/)",
	"description": "A fully type-safe i18n library specifically designed for partial hydration patterns like Astro's islands.",
	"homepage": "https://inlang.com/m/iljlwzfs/paraglide-astro-i18n",
	"repository": {
		"type": "git",
		"url": "https://github.com/opral/inlang-paraglide-js"
	},
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"test": "vitest run --passWithNoTests --dir src",
		"build": "tsc",
		"dev": "tsc -w",
		"lint": "eslint ./src --fix",
		"format": "prettier ./src --write",
		"clean": "rm -rf ./dist ./node_modules"
	},
	"dependencies": {
		"@inlang/paraglide-vite": "workspace:*"
	},
	"peerDependencies": {
		"astro": "^4 || ^5"
	},
	"keywords": [
		"paraglide",
		"javascript i18n",
		"i18n",
		"l10n",
		"translation",
		"internationalization",
		"internationalisation",
		"localization",
		"lint",
		"astro-integration",
		"astro",
		"astro i18n",
		"seo",
		"accessibility",
		"withastro"
	],
	"devDependencies": {
		"typescript": "^5.5.4"
	}
}
