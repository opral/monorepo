{
    "name": "@inlang/paraglide-js-adapter-next",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "build": "node ./build.mjs",
        "test": "vitest run --passWithNoTests"
    },
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./middleware": {
            "import": "./dist/app/middleware.js",
            "types": "./dist/app/middleware.d.ts"
        },
        "./navigation": {
            "import": "./dist/app/navigation/index.js",
            "types": "./dist/app/navigation/index.d.ts"
        },
        "./pages": {
            "import": "./dist/pages/entry.js",
            "types": "./dist/pages/index.d.ts"
        },
        "./plugin": {
            "require": "./dist/plugin/index.cjs",
            "types": "./dist/plugin/index.d.ts"
        }
    },
    "keywords": [
        "inlang",
        "i18n",
        "localization",
        "translation",
        "internationalization",
        "next.js",
        "next",
        "react"
    ],
    "files": [
        "dist"
    ],
    "peerDependencies": {
        "next": "^13.0.0",
        "react": "^18"
    },
    "devDependencies": {
        "@inlang/sdk": "workspace:*",
        "@rollup/plugin-commonjs": "^25.0.7",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-typescript": "^11.1.5",
        "@types/react": "^18",
        "rollup": "3.29.1",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "rollup-preserve-directives": "^1.1.0",
        "typescript": "^5.3.3"
    }
}