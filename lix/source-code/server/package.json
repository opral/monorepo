{
	"name": "@lix-js/server",
	"type": "module",
	"private": true,
	"exports": {
		".": "./dist/index.js"
	},
	"files": [
		"./dist",
		"./src"
	],
	"scripts": {
		"build": "tsc --build",
		"test": "tsc --noEmit && vitest run --passWithNoTests --coverage",
		"dev": "node --loader tsx ./src/main.ts",
		"production": "NODE_ENV=production tsx ./src/main.ts",
		"lint": "eslint ./src --fix",
		"format": "prettier ./src --write",
		"clean": "rm -rf ./dist ./node_modules"
	},
	"dependencies": {
		"@inlang/env-variables": "*",
		"@inlang/telemetry": "*",
		"@isomorphic-git/cors-proxy": "2.7.1",
		"@sentry/node": "^7.47.0",
		"@sentry/tracing": "^7.47.0",
		"compression": "^1.7.4",
		"cookie-session": "2.0.0",
		"express": "^4.18.2",
		"jose": "4.15.4",
		"octokit": "3.1.0",
		"tsx": "3.12.8"
	},
	"devDependencies": {
		"@types/compression": "^1.7.2",
		"@types/cookie-session": "2.0.45",
		"@types/express": "^4.17.17",
		"@vitest/coverage-v8": "0.34.3",
		"typescript": "5.2.2",
		"vitest": "0.34.3"
	},
	"license": "Apache-2.0"
}
