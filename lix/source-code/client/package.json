{
	"name": "@lix-js/client",
	"version": "0.5.0",
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"exports": {
		".": "./dist/index.js",
		"./src/*.ts": "./src/*.ts",
		"./src/browser-auth.ts": "./src/browser-auth.ts"
	},
	"types": "./dist/index.d.ts",
	"files": [
		"./dist",
		"./src"
	],
	"scripts": {
		"build": "tsc --build",
		"dev": "tsc --watch",
		"test": "vitest run --coverage --test-timeout 60000",
		"bench": "vitest bench --watch=false",
		"lint": "eslint ./src --fix",
		"format": "prettier ./src --write",
		"clean": "rm -rf ./dist ./node_modules"
	},
	"dependencies": {
		"@lix-js/fs": "workspace:*",
		"@octokit/types": "12.4.0 ",
		"isomorphic-git": "git@github.com:martin-lysk/isomorphic-git.git#build",
		"octokit": "3.1.2",
		"solid-js": "1.7.11"
	},
	"license": "Apache-2.0",
	"devDependencies": {
		"@vitest/coverage-v8": "1.1.1",
		"typescript": "5.2.2",
		"vitest": "1.1.1"
	}
}
