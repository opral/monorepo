{
  "entryPoints": ["../lix-sdk/src/index.ts"],
  "out": "docs/api/reference",
  "excludePrivate": true,
  "excludeExternals": true,
  "excludeInternal": true,
  "plugin": ["typedoc-plugin-markdown", "typedoc-vitepress-theme"],
  "readme": "none",
  "sort": ["alphabetical"],
  "hideGenerator": true,
  "entryPointStrategy": "expand",
  "tsconfig": "./typedoc.tsconfig.json",
  "gitRemote": "origin",
  "gitRevision": "main",
  "sourceLinkTemplate": "https://github.com/inlang/monorepo/blob/{gitRevision}/{path}#L{line}",
  "visibilityFilters": {
    "protected": false,
    "private": false,
    "inherited": true,
    "external": false
  }
}
