{
  "name": "@lix-js/agent",
  "version": "0.1.0",
  "description": "Interactive CLI agent for Lix change control system",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "browser": "dist/browser.js",
  "types": "dist/index.d.ts",
  "bin": {
    "lix-agent": "bin/run.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "browser": "./dist/browser.js",
      "types": "./dist/index.d.ts"
    }
  },
  "type": "module",
  "scripts": {
    "build": "tsc && npm run build:browser",
    "build:browser": "tsc -p tsconfig.browser.json",
    "start": "node dist/main.js",
    "dev": "tsc -w",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "clean": "rm -rf dist",
    "prebuild": "pnpm run clean",
    "prepublish": "pnpm run build",
    "lix": "node bin/run.js",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "lix",
    "cli",
    "agent",
    "llm",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@lix-js/plugin-csv": "workspace:*",
    "@lix-js/plugin-json": "workspace:*",
    "@lix-js/sdk": "workspace:*",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "diff": "^5.1.0",
    "kysely": "^0.27.0",
    "nanoid": "^5.0.4",
    "openai": "^4.24.1",
    "papaparse": "^5.4.1",
    "readline-sync": "^1.4.10"
  },
  "devDependencies": {
    "@types/diff": "^5.0.8",
    "@types/node": "^20.10.6",
    "@types/readline-sync": "^1.4.8",
    "@vitest/coverage-v8": "^1.1.3",
    "typescript": "^5.3.3",
    "vitest": "^1.1.3"
  }
}
