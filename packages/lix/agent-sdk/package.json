{
  "name": "@lix-js/agent-sdk",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "description": "Minimal Lix agent built on the Vercel AI SDK.",
  "license": "Apache-2.0",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc --build",
    "dev": "tsc --watch",
    "test": "vitest run --typecheck",
    "test:watch": "vitest --typecheck",
    "format": "prettier ./src --write",
    "lint": "oxlint --config .oxlintrc.json \"src/**/*.{js,ts}\" --fix",
    "clean": "rm -rf ./dist ./node_modules"
  },
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.12",
    "@ai-sdk/provider": "^2.0.0",
    "@lix-js/sdk": "workspace:*",
    "ai": "^5.0.34",
    "dedent": "^1.7.0",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@lix-js/plugin-md": "workspace:*",
    "@opral/tsconfig": "workspace:*",
    "oxlint": "^1.14.0",
    "prettier": "^3.4.1",
    "typescript": "^5.5.4",
    "vitest": "4.0.0-beta.10"
  }
}
